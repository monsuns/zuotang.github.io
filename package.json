{
  "name": "gst-mobile",
  "version": "1.0.0",
  "description": "gstzy.cn mobile",
  "scripts": {
    "start": "cross-env RUN_ENV=local NODE_ENV=development webpack-dev-server --watch-poll --progress --hotOnly  --mode development --config  ./webpack.dev.js",
    "devStart": "cross-env RUN_ENV=dev NODE_ENV=development webpack-dev-server --watch-poll --progress --hotOnly  --mode development --config  ./webpack.dev.js",
    "stgStart": "cross-env RUN_ENV=stg NODE_ENV=development webpack-dev-server --watch-poll --progress --hotOnly  --mode development --config  ./webpack.dev.js",
    "testStart": "cross-env RUN_ENV=test NODE_ENV=development webpack-dev-server --watch-poll --progress --hotOnly  --mode development --config  ./webpack.dev.js",
    "server": "cross-env RUN_ENV=production NODE_ENV=production babel-node --inspect --harmony   server/index.js",
    "devServer": "cross-env RUN_ENV=dev NODE_ENV=production babel-node --inspect --harmony   server/index.js",
    "stgServer": "cross-env RUN_ENV=stg NODE_ENV=production babel-node --inspect --harmony   server/index.js",
    "testServer": "cross-env RUN_ENV=test NODE_ENV=production babel-node --inspect --harmony   server/index.js",
    "build": "cross-env RUN_ENV=build NODE_ENV=production webpack --mode production --config ./webpack.build.js  --optimize-minimize",
    "server-dev": "cross-env RUN_ENV=server NODE_ENV=production nodemon  --watch src --watch server --exec babel-node  --harmony  server/index.js",
    "move": "rm -rf www && cp -r build www",
    "publish": "npm run build && npm run move && pm2 reload"
  },
  "author": "",
  "license": "ISC",
